            $('.docLogo').show();
          }
        }
      });
      document.addEventListener("click", (evt) => {
        const flyoutElement = document.getElementById("gsc-i-id1");
        const flyoutElement2 = document.getElementById("gs_cb50");
        const flyoutElement3 = document.querySelector(".gsc-search-button-v2");
        const flyoutElement4 = document.getElementById("search3");

        let targetElement = evt.target; 

        if (!(targetElement == flyoutElement || targetElement == flyoutElement2 || targetElement == flyoutElement3 || targetElement == flyoutElement4)) {
          
          if ($('#gsc-i-id1').is(":visible")) {
            $("#gsc-i-id1").val('');
            $("#gsc-i-id1").hide();
            $("#gsc-iw-id1").hide();
            $('.docLogo').show();
          }
        }
      });
    }
    else {
      document.getElementById('search2').removeAttribute('data-action');
      document.querySelector('#gsc-i-id1').placeholder = 'Search';
      $("#gsc-iw-id1").hide();
      $(".gsc-search-button-v2").bind({
        click: function () {
          
          if ($('#gsc-i-id1').is(":hidden")) {
            $("#gsc-iw-id1").show();
            $('.docLogo').hide();
            $("#gsc-i-id1").show();
          }
          else {
            $("#gsc-i-id1").hide();
            $('.docLogo').show();
            $("#gsc-iw-id1").hide();
          }
        },
      });
      $('#search2').click(function () {
        $(".gsc-search-button-v2").click();
        
      })
      $("#gs_cb50").bind({
        click: function () {
          $("#gsc-iw-id1").hide();
          $('.docLogo').show();
        },
      });
      document.addEventListener("keyup", function (event) {
        if (event.keyCode === 13) {
          if ($('.gsc-results-wrapper-overlay').is(":visible")) {
            
            $("#gsc-i-id1").hide();
            $('.docLogo').show();
          }
        }
      });
      document.addEventListener("click", (evt) => {
        const flyoutElement = document.getElementById("gsc-i-id1");
        const flyoutElement2 = document.getElementById("gs_cb50");
        const flyoutElement3 = document.querySelector(".gsc-search-button-v2");
        const flyoutElement4 = document.getElementById("search2");

        let targetElement = evt.target; 


        if (!(targetElement == flyoutElement || targetElement == flyoutElement2 || targetElement == flyoutElement3 || targetElement == flyoutElement4)) {
          
          if ($('#gsc-i-id1').is(":visible")) {
            $("#gsc-i-id1").val('');
            $("#gsc-i-id1").hide();
            $("#gsc-iw-id1").hide();
            $('.docLogo').show();
          }
        }
      });
    }
  }
  

  
  var $animation_elements = $('.animation-element');
  var $window = $(window);

  function check_if_in_view() {
    var window_height = $window.height();
    var window_top_position = $window.scrollTop();
    var window_bottom_position = (window_top_position + window_height);

    $.each($animation_elements, function () {
      var $element = $(this);
      var element_height = $element.outerHeight();
      var element_top_position = $element.offset().top;
      
      
      var element_bottom_position = (element_top_position + element_height);

      if ((element_bottom_position >= window_top_position) &&
        (element_top_position <= window_bottom_position)) {
        $element.addClass('in-view');
        
      } else {
        $element.removeClass('in-view');
      }
    });
  }
  $window.on('scroll resize', check_if_in_view);
  $window.trigger('scroll');

  
  

  $(".main-top-bar .nav a").click(function () {
    if (document.querySelector(".bi-caret-down-fill").style.transform == "rotate(270deg)") {
      document.querySelector(".bi-caret-down-fill").style.transform = "none";
    } else {
      document.querySelector(".bi-caret-down-fill").style.transform = "rotate(270deg)";
    }
  });


  document.addEventListener("click", (evt) => {

    const clickedElement1 = document.querySelector(".nav-item.nav-link.dropdown-toggle b");
    const clickedElement2 = document.querySelector(".bi-caret-down-fill");
    const clickedElement3 = document.querySelector(".nav-link.dropdown-item");

    let targetElement = evt.target;

    
    

    if (targetElement == clickedElement1) {
      
    }
    else if (targetElement == clickedElement3) {
      
    }
    else if (targetElement.parentNode == clickedElement2) {
      
    }
    else {
      
      if ($("#releaseddm").is(":hidden")) {
        clickedElement2.style.transform = "none";
      }
    }
  });





  

  
  
  
  

  $("#main-nav").on("click", function () {
    setTimeout(() => {
      window.scrollTo(window.scrollX, window.scrollY - 50);
    }, 0100);
    
  });

  $(window).on("scroll", function () {
    
    var h2All = document.querySelectorAll('#main-nav a'); 
    $('#main-nav a').each(function (i) {
      if ($(this).is('.active')) {
        

        let h2href = h2All[i].getAttribute('href'); 
        

        h2get = document.querySelectorAll(`[data-clipboard-text='${h2href}']`); 
        

        h2parent = h2get[0].parentNode; 
        

        h3child = $(h2parent).nextUntil().filter('h2, h3, h4, h5, h6'); 
        

        t = document.querySelector('#toc');

        let subCount = h3child.length;

        
        
        
        
        
        
    
        t.innerHTML = ``;

        if(subCount){
          for (let i = 0; i < subCount; i++) {
            if ($(h3child[i]).prop("tagName") == "H2") {
              if ((h3child[i].innerText).includes(":")) {
                t.innerHTML += `<a href="#${h3child[i].id}" class="right-toc-h2">${h3child[i].innerText.split(":")[0] + ":" + "\n" + h3child[i].innerText.split(":")[1]}</a>`;
              }
              else {
                t.innerHTML += `<a href="#${h3child[i].id}" class="right-toc-h2">${h3child[i].innerText}</a>`;
              }
            }
            if ($(h3child[i]).prop("tagName") == "H3") {
              if ((h3child[i].innerText).includes(":")) {
                t.innerHTML += `<a href="#${h3child[i].id}" class="right-toc-h3">${h3child[i].innerText.split(":")[0] + ":" + "\n" + h3child[i].innerText.split(":")[1]}</a>`;
              }
              else {
                t.innerHTML += `<a href="#${h3child[i].id}" class="right-toc-h3">${h3child[i].innerText}</a>`;
              }
            }
            if ($(h3child[i]).prop("tagName") == "H4") {
              if ((h3child[i].innerText).includes(":")) {
                t.innerHTML += `<a href="#${h3child[i].id}" class="right-toc-h4">${h3child[i].innerText.split(":")[0] + ":" + "\n" + h3child[i].innerText.split(":")[1]}</a>`;
              }
              else {
                t.innerHTML += `<a href="#${h3child[i].id}" class="right-toc-h4">${h3child[i].innerText}</a>`;
              }
            }
            if ($(h3child[i]).prop("tagName") == "H5") {
              if ((h3child[i].innerText).includes(":")) {
                t.innerHTML += `<a href="#${h3child[i].id}" class="right-toc-h5">${h3child[i].innerText.split(":")[0] + ":" + "\n" + h3child[i].innerText.split(":")[1]}</a>`;
              }
              else {
                t.innerHTML += `<a href="#${h3child[i].id}" class="right-toc-h5">${h3child[i].innerText}</a>`;
              }
            }
            if ($(h3child[i]).prop("tagName") == "H6") {
              if ((h3child[i].innerText).includes(":")) {
                t.innerHTML += `<a href="#${h3child[i].id}" class="right-toc-h6">${h3child[i].innerText.split(":")[0] + ":" + "\n" + h3child[i].innerText.split(":")[1]}</a>`;
              }
              else {
                t.innerHTML += `<a href="#${h3child[i].id}" class="right-toc-h6">${h3child[i].innerText}</a>`;
              }
            }
          }
        }
        else{
          t.innerHTML = '<span class="right-toc-empty">No subtopics in this<br>section</span>';
        }
        
        
        
        
        for (let i = 0; i < subCount; i++) {
          let currentElement = h3child[i];
          let j = (i + 1) % (subCount);
          if (j == 0) {
            checkTocLastElement(currentElement.id);
            { break; }
          }
          let nextElement = h3child[j];
          checkToc(currentElement.id, nextElement.id);
        }
      }
      else {
        
      }
    });
  });

  function checkToc(id, nextid) {
    var myElement = document.getElementById(id);
    var myNextElement = document.getElementById(nextid);
    var bounding = myElement.getBoundingClientRect();
    var nextbounding = myNextElement.getBoundingClientRect();
    if (bounding.bottom <= 188) {
      if (nextbounding.top >= 212) {
         
        
        
        var div = document.getElementById("toc");
        
        
        
        
        
        
        
        
        var txt = document.querySelector(`a[href="#${id}"]`).text;
        $(".breadcrumb-item.currenth3").text(txt);
        
        
        document.querySelector(`a[href="#${id}"]`).id = "current";
        document.querySelector(`a[href="#${id}"]`).scrollIntoView({
          alignToTop: "true",
          behavior: 'smooth'
        });
        
        
        document.querySelector(`a[href="#${id}"]`).classList.add('right-toc-active');
        
        
        
        
      }
      else {
        
      }
    }
    else {
      
    }

  }

  

  function checkTocLastElement(id) {
    var myElement = document.getElementById(id);
    var bounding = myElement.getBoundingClientRect();
    if (bounding.top <= 188) {
       
      
      var div = document.getElementById("toc");
      

      var lastTxt = document.querySelector(`a[href="#${id}"]`).text;
      $(".breadcrumb-item.currenth3").text(lastTxt);
      
      
      
      document.querySelector(`a[href="#${id}"]`).classList.add('right-toc-active');
      
      
      
      
    }
    else {
      
    }
  }

  
  $("#toc").click(function (e) {
    $(".breadcrumb-item.currenth3").hide();
    setTimeout(() => {
      window.scrollTo(window.scrollX, window.scrollY - 177);
    }, 050);
    setTimeout(() => {
      let url = $(e.target).attr("href");
      
      document.querySelector(`a[href="${url}"]`).classList.add('right-toc-active');
      
      
      
      
      
    }, 0200);
  });





























































































</script>

  </body>
</html>
