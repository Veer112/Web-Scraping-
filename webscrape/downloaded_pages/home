          id="mob_comments" name="mob_comments" 
          rows="5" cols="35" 
          placeholder="Enter your comments here." 
          class="mob-form-control" 
          maxlength=2000
          onfocus="mobileOnChangeHandler()"
          onkeyup="mobileOnChangeHandler()"></textarea>
      </div>
      
      
      <div id="message"></div>

      <div class="mob_form-btn-container">
        <button type="reset" class="clear" type="button" id="mob_cancel" onclick="mobileResetForm()">Cancel</button>
        <button type="submit" class="submit" type="button" id="mob_submit" onclick="mobileValidateForm(event)">Submit</button>
      </div>
    </div>
  </form>
</div>


<script type="text/javascript">

  $(document).ready(() => {

    clearFormData();
    hideFeedbackHandler();

    mobileResetForm();
    mobileHideFeedbackHandler();

    
    
    
    
    $('#feedback-tab').click(()=>{
      $('#feedback-form').fadeToggle('fast',() => $('#feedback-tab').toggleClass('down-arrow'));
    });

    $('#mob_feedback-tab').click(()=>{
      $('#mob_feedbackform').fadeToggle('fast',() => $('#mob_feedback-tab').toggleClass('down-arrow'));
    });

    
    let prevScrollpos = window.pageYOffset;
    window.addEventListener('scroll',() => {
      var currentScrollPos = window.pageYOffset;
      if (prevScrollpos != currentScrollPos) {
        $("#feedback-form").hide();
        $("#feedback-tab").removeClass('down-arrow');
      }
    });

    
    $('input[type="radio"]#specific').on('change',(e) => {
      if(e.target.checked){
        
        document.getElementById("copiedText").value = '';
        document.getElementById("guidetext").innerHTML = 'Please select the text of concern to enter your comments';
        
        $("textarea#comments").prop('disabled', true);

        
        $('#feedback-form').removeClass('article');
        $('#feedback-form').addClass('specific');

        
        $(".main-content, .main-content-2").on('mousedown',hideFeedbackHandler);

        
        document.addEventListener('mouseup',copySelectedText);

        
        $("#feedback-form").on('mousedown',stopPropagationHandler);
        $("#feedback-tab").on('mousedown',stopPropagationHandler);

        
        formChangeHandler()
      }
    });

    
    $('input[type="radio"]#article').on('change',(e) => {
      if(e.target.checked){

        
        document.getElementById("copiedText").value = '';
        document.getElementById("guidetext").innerHTML = 'Please select the text of concern to enter your comments';
        
        $("textarea#comments").prop('disabled', false);

        
        $('#feedback-form').addClass('article');
        $('#feedback-form').removeClass('specific');

        
        $(".main-content, .main-content-2").off('mousedown',hideFeedbackHandler);
        document.removeEventListener('mouseup',copySelectedText);
        $("#feedback-form").off('mousedown',stopPropagationHandler);
        $("#feedback-tab").off('mousedown',stopPropagationHandler);

        
        formChangeHandler();
      }
    });
    
  });

  function stopPropagationHandler(e){
    e.stopPropagation();
  }

  
  function hideFeedbackHandler(){
    $("#feedback-form").hide(); 
    $("#feedback-tab").removeClass('down-arrow');
  }

  function showFeedbackHandler(){
    $("#feedback-form").show();
    $('#feedback-tab').addClass('down-arrow');
  }

  function copySelectedText() {
    
     let thetext = getTextSelectionHandler();
     if (thetext.length >= 1) {
      
      document.getElementById("copiedText").value = thetext;
      document.getElementById("guidetext").innerHTML = thetext;
      $("textarea#comments").prop('disabled', false);
      showFeedbackHandler();
    }

    
    formChangeHandler();
  }

  function getTextSelectionHandler() {
    let selectedText = "";
    if (window.getSelection().containsNode(document.body, true)) {
      selectedText = window.getSelection().toString();
    }
    return selectedText;
  }

  function clearText(){
    document.getElementById("copiedText").value = '';
    document.getElementById("guidetext").innerHTML = 'Please select the text of concern to enter your comments';
    $("textarea#comments").prop('disabled', true);
    formChangeHandler();
  }

  
  function clearFormData() {
    document.getElementById("feedback-form").reset();
    $('[name="response"]:checked').each((pos,ele) => ele.checked = false)
    $('[name="feedbackoption"]:checked').each((pos,ele) => ele.checked = false);
    document.getElementById('copiedText').value = '';
    document.getElementById("guidetext").innerHTML = 'Please select the text of concern to enter your comments';
    document.getElementById('comments').value = '';
    document.getElementById('userEmail').value = '';
    $(".main-content, .main-content-2").off('mousedown',hideFeedbackHandler);
    document.removeEventListener('mouseup',copySelectedText);
    $("#feedback-form").off('mousedown',stopPropagationHandler);
    $("#feedback-tab").off('mousedown',stopPropagationHandler);
    $('#feedback-form').removeClass('yes-response no-response partly-response specific article');
    document.getElementById('loading').innerHTML = "";
    $('#comments').prop('disabled', true);
    $('#submit').prop('disabled', true);
  }

  
  
  function countChars(ele){
    let maxLength = 2000;
    let strLength = ele.value.length;
    let infoText;

    if(strLength == 0){
      infoText = '2000 characters remaining';
    }
    else if (strLength > maxLength) {
      infoText = strLength + ' out of ' + maxLength + ' characters';
    } else {
      infoText = (maxLength - strLength) + ' characters remaining';
    }

    document.getElementById("charNum").innerHTML = infoText;
    formChangeHandler();
  }

  
  function responseChangeHandler(event,addClass,removeClass){
    if(event.target.checked){
      $('#feedback-form').removeClass(removeClass);
      $('#feedback-form').addClass(addClass);
    }
  }

  
  function FormUIValidationHandler(){
    let validationState = false;
    let responseValue = $('[name="response"]:checked').length?$('[name="response"]:checked')[0].value:'';
    let feedbackScope = $('[name="feedbackoption"]:checked').length?$('[name="feedbackoption"]:checked')[0].value:'';
    let copiedText = document.getElementById('copiedText').value;
    let commentsText = document.getElementById('comments').value;
    let email = document.getElementById('userEmail').value;

    if(responseValue === 'yes'){
      validationState = true;
    }
    else if(responseValue === 'partly' || responseValue === 'no'){
      if(feedbackScope === 'article' && commentsText.length) validationState = true;
      else if (feedbackScope === 'specific' && copiedText.length && commentsText.length) validationState = true;
    }

    
    return validationState;
  }

  
  function formChangeHandler(e){
    e? e.stopPropagation() : '';
    let validityState = FormUIValidationHandler();
    if(validityState) $('#submit').prop('disabled', false);
    else $('#submit').prop('disabled', true);
    document.querySelector('#loading').innerHTML = '';
  }

  function getPageURL() {
    return window.location.href;
  }

  function feedbackSubmitHandler(e){
    e.preventDefault();
    sendResponse();
  }

  function sendResponse() {

    let validationState = FormUIValidationHandler();
    if(!validationState){
      document.querySelector('#loading').innerHTML = '<span style="color: #D22D3D;">Invalid input, please check your responses and try again</span>'
    }
    else{
      document.querySelector('#loading').innerHTML = '';
    }

    let responsevalue = $('[name="response"]:checked').length?$('[name="response"]:checked')[0].value:'';
    let feedbackoption = $('[name="feedbackoption"]:checked').length?$('[name="feedbackoption"]:checked')[0].value:'';
    let feedback_on = '';
    if (feedbackoption == "specific") {
      feedback_on = "Specific content in this article.";
    } else if (feedbackoption == "article") {
      feedback_on = "This article or documentation in general.";
    }
    let copiedtext = document.getElementById('copiedText').value;
    let comments = document.getElementById('comments').value;
    if (window.location.href.indexOf("home") > 0) {
      var article_title = "Product landing page";
    }
    else {
      var article_title = document.getElementsByTagName("h2")[0].innerHTML;
    }
    var email = document.getElementById('userEmail').value;
    var product = getProductName();
    var pageURL = getPageURL();

    $.ajax({
      type: "POST",
      url: 'https://docfeedback.int-aws-us.webmethods.io/runflow/run/sync/1RHiXt9MXS',
      data: {
        article_title: article_title,
        responsevalue: responsevalue,
        feedbackoption: feedback_on,
        copiedtext: copiedtext,
        comments: comments,
        email: email,
        product: product,
        pageURL: pageURL
      },
      success: function (res) {
        
        clearFormData();
        $('#loading').html("<span class='container alert alert-success'>Thank you! We value your feedback.</span>");
        setTimeout(function () {
          document.getElementById('loading').innerHTML = "";
          hideFeedbackHandler();
        }, 2000); 
      }
    });
  }
</script>


<script>
  


  
  function mobileHideFeedbackHandler(event=null){
    if(event) event.preventDefault();
    $("#mob_feedbackform").hide();
    $("#mob_feedback-tab").removeClass('down-arrow');
    document.getElementById("message").classList.remove('alert-error');
    document.getElementById("message").classList.remove('alert-success');
    document.getElementById("message").innerHTML = "";
  }

  
  function mobileShowFeedbackHandler(){
    $("#mob_feedbackform").show();
    $('#mob_feedback-tab').addClass('down-arrow');
  }

  function mobileOnChangeHandler(event){
    document.getElementById("message").classList.remove('alert-error');
    document.getElementById("message").classList.remove('alert-success');
    document.getElementById("message").innerHTML = "";
  }

  
  
  function mobileResetForm() {
    
    document.getElementById("mob_comments").value = "";
    document.getElementById("message").classList.remove('alert-error');
    document.getElementById("message").classList.remove('alert-success');
    document.getElementById("message").innerHTML = "";
    document.querySelectorAll('input[name="mob_responseValue"]').forEach( e => e.checked = false);
    $("#mob_feedbackform").hide();
    $("#mob_feedback-tab").removeClass('down-arrow');
  }

  
  
  
  function mobileValidateForm(e) {
    e.preventDefault();
    let mobForm = new FormData(document.getElementById("mob_feedbackform"));
    let mobComments = mobForm.get("mob_comments");
    let mobResponseValue = mobForm.get("mob_responseValue");
    let messageBox = document.getElementById('message');

    let noResponse = ! Boolean(mobResponseValue); 
    let noComment = ! Boolean(mobComments.trim().length); 

    if( noResponse || (mobResponseValue === 'no' && noComment ) ){
      
      
      

      messageBox.classList.add('alert-error');
      messageBox.classList.remove('alert-success');
      messageBox.innerHTML = "Invalid input, please check your responses and try again";
    }
    else{
      messageBox.innerHTML = "";
      
      mobileSendResponse();
    }
  }

  
  function mobileSendResponse() {
    let product = getProductName();
    let mobForm = new FormData(document.getElementById("mob_feedbackform"));
    let mob_comments = mobForm.get("mob_comments");
    let mob_responseValue = mobForm.get("mob_responseValue") || '';

    let article = document.getElementsByTagName("h1");
    let productName = document.querySelector('.title-product');
    let article_title = article.length ? article[0].innerHTML : (productName?( productName.innerHTML + ' Product Welcome Page'):'Main landing page');

   $.ajax({
     type: "POST",
     url: 'https://docfeedback.int-aws-us.webmethods.io/runflow/run/sync/1shkZsCOwc',
     data: {
       article_title: article_title,
       responsevalue: mob_responseValue,
       comments: mob_comments,
       product: product
     },
     success: function (res) {
       
       

       
        document.getElementById("mob_comments").value = "";
        document.getElementById("message").classList.remove('alert-error');
        document.getElementById("message").classList.remove('alert-success');
        document.getElementById("message").innerHTML = "";
        document.querySelectorAll('input[name="mob_responseValue"]').forEach( e => e.checked = false);

        document.getElementById("message").classList.add('alert-success');
        document.getElementById("message").classList.remove('alert-error');
        document.getElementById('message').innerHTML = "Thank you! We value your feedback.";

        setTimeout(function () {
          document.getElementById("message").classList.remove('alert-error');
          document.getElementById("message").classList.remove('alert-success');
          document.getElementById("message").innerHTML = "";
          mobileHideFeedbackHandler();
        }, 3000);
     }
   });
 }

 


 

 
 function getProductName() {
  
  var product = String(window.location.pathname.split("/")[1]);
  if (product == "api") {
    product = "API";
  } else if (product == "integration") {
    product = "Integration";
  } else if (product == "b2b") {
    product = "B2B";
  }
  return product;
}

 
</script>


