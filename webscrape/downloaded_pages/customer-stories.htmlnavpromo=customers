<script>
    var en_corp= "/en_corporate"; 
    var pageURL= window.location.href;
    if(pageURL.includes(en_corp)){
        var pagelocale = "en-US"; //if en-corporate,then locale is default en-US
    }
    else{
        var pagelocale = 'en\u002DUS';
    }
   
    window.digitalData = window.digitalData || {};
    window.digitalData.page = window.digitalData.page || {};
    window.digitalData.page.attributes = window.digitalData.page.attributes || {};
    window.digitalData = {
        page: {
            pageInfo: {
                pageName: 'Customer Success Stories \x26 Case Studies | Software AG',
                pageURL: window.location.href,
                pageRef: document.referrer
            },
            category: {
                productCategory: 'Customers',
                productName: 'Cumulocity IoT',
                productCapabilities: 'webMethods,webMethods.io,Adabas,Natural,ARIS,Alfabet,TrendMiner,CONNX,IoT \x26 Analytics,Integration and APIs,Analytics,IT Transformation,Process Transformation,Adabas \x26 Natural',
                productUsageLevel: '',
                campaignLandingPage: 'false',
                pageType: 'hero'
            },
            attributes: {
                locale: pagelocale,
                domain: window.location.hostname,
                language: pagelocale.split("-")[0],
                country: pagelocale.split("-")[1],
                source: 'aem',
                version: '17.0.0'
            },
            components: []

        }
    }
</script>




