       <a data-action="remove" data-name="cf" data-value="{{name}}" style="font-weight:bold;">{{label}} ({{matchingDocumentCount}})</a>
       {{else}}
       <a data-action="add" data-name="cf" data-value="{{name}}">{{label}} ({{matchingDocumentCount}})</a>
       {{/isFilterActive}}
   </li>
   {{/each}}
</ul>
</script>

