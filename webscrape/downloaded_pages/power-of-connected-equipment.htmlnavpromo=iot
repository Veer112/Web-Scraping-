   <input type="hidden" name="cn" />
   <button class="header__search-form-submit a-icon a-icon--hover" type="submit" tabindex="-1">
       <svg class="a-icon a-icon--search">
           <use xlink:href="#search"></use>
       </svg>
   </button>
</form>
<!--End: ig/query-->
</script>

<!-- search result output -->
<script name="ig_search" type="text/x-handlebars-template">
<!--Begin: ig/search-->
{{#if values.length}}
{{#if correctedQuery}}
<p class="proposed-query">{{i18n 'dym'}}:&nbsp;<i><a data-action="change" data-name="q" data-value="{{correctedQuery}}">{{correctedQuery}}</a></i></p>
{{/if}}

<div class="counter">{{i18n 'hits_for_search' query=searchParams.parameters.query count=matchingDocumentCount first=@root.pager.firstPageHit last=@root.pager.lastPageHit}}</div>

<br clear="all"/>

{{#each values}}
{{>result}}
{{/each}}

{{>related}}

{{>pager}}
{{else}}
{{#if searchParams.parameters.query}}
<p>
   {{i18n 'no_hits' query=searchParams.parameters.query}}<br>
   {{i18n 'no_hits_check_query'}}
</p>
{{#if correctedQuery}}
<p class="proposed-query">{{i18n 'dym'}}:&nbsp;<i><a data-action="change" data-name="q" data-value="{{correctedQuery}}">{{correctedQuery}}</a></i></p>
{{/if}}
{{else}}
<p>{{i18n 'please_query_enter_value'}}</p>
{{/if}}
{{/if}}
<!--End: ig/search-->
</script>

