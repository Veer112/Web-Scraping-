        <!-- Common Properties for all product teaser cards below-->
        <!-- Optional Caption -->
        <!-- Container exclusive to product teaser B for body column -->
        <div class=" teasercontent">

            
            
            <h6 class="a-overline">Solution</h6>


            <!-- Heading -->
            <h5 class="a-h5">
