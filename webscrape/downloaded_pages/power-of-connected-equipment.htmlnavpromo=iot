<script type="text/javascript" src="/etc.clientlibs/softwareag/clientlibs/clientlib-site.min.ACSHASHf538cabf6b1ed3c6271a9e11464b381c.js" defer></script><script type="text/javascript" src="/libs/dam/components/scene7/common/clientlibs/viewer.min.ACSHASH4618b42d2de5cb16b5ebca7fdf77812d.js" defer></script><script type="text/javascript" src="/libs/dam/components/scene7/dynamicmedia/clientlibs/dynamicmedia.min.ACSHASH1c0d64a1181f9fc4d177d404992b6b05.js" defer></script><script type="text/javascript" src="/libs/dam/components/scene7/common/clientlibs/i18n.min.ACSHASH68da31d5bd4fb4fe82646d5b80611425.js" defer></script>



<script type="text/javascript">
	function hidefooter() {	
           if(!$('.footer').children().length)
        {
            $('.footer').css("padding","0");
          }

	}

    function removeHeaderZindex(){
        
        if(!$('.header-v1').children().length){
            $('.header-v1').css("z-index","unset"); 
        }
    }

    if (document.readyState === 'loading') {  

        document.addEventListener('DOMContentLoaded', hidefooter);
        document.addEventListener('DOMContentLoaded', removeHeaderZindex);
    }
     else {  
    
            hidefooter();
            removeHeaderZindex();
    }

</script>

<script name="ig_query" type="text/x-handlebars-template">
<!--Begin: ig/query-->
