$(document).ready(function () {
   $('body').igGlobalSearch({
       "uiLang": "en",
       "searchLang" : "en",
       "url" : "https://search.softwareag.com/web/api/1.1/rpc/search/search",
       "suggestUrl" : "https://search.softwareag.com/web/api/1.1/rpc/search/suggest",
       "timeZoneId" : Intl.DateTimeFormat().resolvedOptions().timeZone,
       "searchResultUrl" : "https://www.softwareag.com/en_corporate/search-result.html"
   });

   $(document).on("click", function () {
       $('input#searchfield').igSuggestions({
           "suggestUrl" : "https://search.softwareag.com/web/api/1.1/rpc/search/suggest",
           "searchLang" : "en"
       });
   });
  
});
</script>
